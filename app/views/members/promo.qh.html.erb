<!--partnerDfmMaindiv start-->
<div id="partnerDfmMaindiv">
  <!--partnerDfmMessagebox start-->
  <div id="partnerDfmMessagebox">
	<!--partnerDfmMessage start-->
    <div id="partnerDfmMessage">
	  <%= image_tag 'dwnarrow.jpg', :size=>'18x18' %>
	  <h2>Deals for Mommy members, get FREE Samples!</h2>
	</div>
    <!--partnerDfmMessage end-->
    <!--partnerDfmViewDealBut start-->
	<div id="partnerDfmViewDealBut"><%= link_to (image_tag 'viewnextdeal-butt.jpg', :size=>'183x44', :alt=>'View Next Deal'), @next_deal %></div>
	<!--partnerDfmViewDealBut end-->
    <!--partnerDeliaTop start-->
	<div id="partnerDeliaTop"><%= image_tag 'dealiatop.png',:size=>'74x92'%></div>
    <!--partnerDeliaTop end-->
  </div>
  <!--partnerDfmMessagebox end-->
  <div class="clear"></div>
  <!--partnerDfmMainContentt end-->
  <div class="partnerDfmMainContentt">
	<!--partnerDfmLeftcol start-->
	<div class="partnerDfmLeftcol">
	  <!--partnerDfmLeftcolHeader start-->
	  <div id="partnerDfmLeftcolHeader"><%= link_to (image_tag 'qh.jpg', :alt=>'Quality Health'), '', :onclick=>"window.open('#{@promo_link}', 'mywindow', 'location=1,status=1,scrollbars=1,width=950,height=600');window.location='#{@next_deal}';return false;" %></div>
	  <!--partnerDfmLeftcolHeader end-->
	  <!--partnerDfmLeftcolMain start-->
	  <div id="partnerDfmLeftcolMain">
		<!--partnerDfmLeftcolContent start-->
		<div class="partnerDfmLeftcolContent">
		  <h2>The Lowdown</h2>
		  <p>
			<strong>Get FREE samples of some of this season’s best healthy products for moms, families, and homes!</strong><br />
		    <ul style="list-style-type: decimal">
              <li><span>Visit the <%=link_to "QualityHealth Winter Samples page by clicking here.",@promo_link,:onclick=>"window.open('#{@promo_link}', 'mywindow', 'location=1,status=1,scrollbars=1,width=950,height=600');window.location='#{@next_deal}';return false;" %></span></li>
              <li><span>Enter your information Code to get started.</span></li>
		      <li><span>Create your QualityHealth profile and answer a few questions to qualify for FREE product samples.<br />Your QualityHealth.com profile also gives you access to special offers, savings, coupons, recipes, and more.

		       </span></li>
            </ul>

          <h2>At a Glance</h2>
            <p>
              Trying something new is a great way to discover products you love.

           </p>
          <p>
            Our FREE Winter Samples are a perfect way to try new products at no cost, including:
          <ul>
            <li><span>Healthy snacks for kids and grownups</span></li>
            <li><span>Bath and skincare products</span></li>
            <li><span> Handy items to help keep a healthy home</span></li>
          </ul>
          <p>
            Discover something you’ll love! Get started now by visiting the Visit the <%=link_to "QualityHealth Winter Samples page!",@promo_link,:onclick=>"window.open('#{@promo_link}', 'mywindow', 'location=1,status=1,scrollbars=1,width=950,height=600');window.location='#{@next_deal}';return false;" %>
          </p>
          </p>
          </p>

		  <div class="clear"></div>
		  <!--partnerDfmLeftColGetStartedBut start-->
		  <blockquote><%= link_to (image_tag 'getstarted_button.png', :alt=>'Get Started',:size=>'206x41'), '', :onclick=>"window.open('#{@promo_link}', 'mywindow', 'location=1,status=1,scrollbars=1,width=950,height=600');window.location='#{@next_deal}';return false;" %><br /><br /><%= link_to 'See Next Deal', @next_deal %></blockquote>
		  <!--partnerDfmLeftColGetStartedBut end-->
		</div>
		<!--partnerDfmLeftcolContent end-->
		<div class="clear"></div>
	  </div>
	  <!--partnerDfmLeftcolMain end-->
    </div>
    <!--partnerDfmLeftcol end-->
    <!--partnerDfmRightcol start-->
    <div class="partnerDfmRightcol">
	  <!--partnerDfmRightcolHeader start-->
	  <div class="partnerDfmRightcolHeader">
		<!--partnerDfmRightcolHeaderTop start-->
		<div class="partnerDfmRightcolHeaderTop"><%= image_tag 'whatdeal-text.gif', :alt=>"What's the Deal?", :size=>'149x38' %></div>
		<!--partnerDfmRightcolHeaderTop end-->
		<dl>
		  <dt>Your Cost:</dt><dd>$0.00</dd>
		  <dt>Value:</dt><dd>$49.95</dd>
		  <dt>Discount:</dt><dd>100%</dd>
		</dl>
		<p class="clear">Pay only Shipping & Handling</p>
	  </div>
	  <!--partnerDfmRightcolHeader end-->
	  <div class="clear"></div>
	  <!--partnerDfmRightcolDealTag start-->
	  <div class="partnerDfmRightcolDealTag">
		<p><strong>view this deal</strong><br /><%= link_to ( image_tag 'get-started.png', :alt=>'Get Started', :size=>'206x41' ) , '', :onclick=>"window.open('#{@promo_link}', 'mywindow', 'location=1,status=1,scrollbars=1,width=950,height=600');window.location='#{@next_deal}';return false;" %></p>
	  </div>
	  <!--partnerDfmRightcolDealTag end-->
	  <div class="clear"></div>
	  <!--partnerDfmRightcolMain start-->
      <div class="partnerDfmRightColImage">

		<%= link_to ( image_tag 'QualityHealth_logo.jpg', :alt=>'Quality Health')  , '', :onclick=>"window.open('#{@promo_link}', 'mywindow', 'location=1,status=1,scrollbars=1,width=950,height=600');window.location='#{@next_deal}';return false;" %><br />

	  </div>
      <div class="partnerDfmRightcolMain">
		<h5>About</h5>
		<p>
          QualityHealth is a great resource for moms, offering awealth of health information, savings, and sample offers.
          <br /><%= link_to 'learn more', '', :onclick=>"window.open('#{@promo_link}', 'mywindow', 'location=1,status=1,scrollbars=1,width=950,height=600');window.location='#{@next_deal}';return false;" %><br />
		</p>
	  </div>
	  <!--partnerDfmRightcolMain end-->
    </div>
    <!--partnerDfmRightcol end-->
  </div>
  <!--partnerDfmMainContentt end-->
  <div class="clear"></div>
  <!--partnerDfmRightcolSocial start-->
  <div class="partnerDfmRightcolSocial">
	<p><strong>Stay in the loop!</strong></p>
	<%= link_to (image_tag 'rightcol-twit.jpg' , :alt=> 'Twitter' , :size=>'22x17'), "http://twitter.com/dealsformommy", :target => "_blank" %>
	<%= mail_to "", (image_tag 'rightcol-mail.jpg' , :alt=> 'Share' , :size=>'22x17'), :subject=>'DealsForMommy.com',:body=>'I want to share with you a great deal site that I found. http://www.dealsformommy.com' %>
	<%= link_to (image_tag 'rightcol-facebook.jpg' , :alt=> 'Facebook' , :size=>'22x17'), "http://facebook.com/dealsformommy", :target => "_blank" %>
	<%= link_to (image_tag 'rightcol-like.jpg' , :alt=> 'Facebook' , :size=>'47x17'), "http://www.facebook.com/ajax/signup_dialog.php?page_id=152488101454439&next=http%3A%2F%2Fwww.facebook.com%2Fdealsformommy", :target => "_blank" %>
  </div>
  <!--partnerDfmRightcolSocial end-->
  <div class="clear"></div>
</div>
<!--partnerDfmMaindiv end-->
